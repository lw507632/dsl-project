<?xml version="1.0" encoding="ASCII"?>
<arduinoml:App xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:arduinoml="http://www.example.org/arduinoml" name="app">
  <bricks xsi:type="arduinoml:Sensor" name="button" pin="10"/>
  <bricks xsi:type="arduinoml:Actuator" name="a1" pin="11"/>
  <bricks xsi:type="arduinoml:Actuator" name="a2" pin="12"/>
  <states name="initial">
    <actions actuator="//@bricks.1" value="LOW"/>
    <actions actuator="//@bricks.2" value="LOW"/>
  </states>
  <states name="a1_on">
    <actions actuator="//@bricks.1"/>
  </states>
  <states name="a2_on">
    <actions actuator="//@bricks.2"/>
    <actions actuator="//@bricks.1" value="LOW"/>
  </states>
  <conditions name="c1" sensors="//@bricks.0">
    <values>HIGH</values>
  </conditions>
  <transitions next="//@states.1" condition="//@conditions.0" previous="//@states.0"/>
  <transitions next="//@states.2" condition="//@conditions.0" previous="//@states.1"/>
  <transitions next="//@states.0" condition="//@conditions.0" previous="//@states.2"/>
</arduinoml:App>
